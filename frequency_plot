#Import matplotlib for histogram functions
import matplotlib.pyplot as plt
#important frequency values from frequency.py
from frequency import freq_reads, freq_writes



# Set values to first line
x = [0, 1] # points on the x axis
y = [freq_reads, freq_writes] # points on the y axis



plt.bar(x,y,)


plt.xlabel('x')
plt.ylabel('y')
plt.title('Frequency of Occurence ')
plt.show()

